import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
        int n=in.nextInt();
		int sum1=0;
		int sum2=0;
		int arr[]=new int[n];
		 for(int i = 0; i < n; i++)
	        {
	            arr[i] = in.nextInt();
	        }
		 for(int j=0;j<n;j++) {
			 if(arr[j]%2==0&&j%2==0) {
				 sum1=sum1+arr[j];
			 }
		if(arr[j]%2!=0&&j%2!=0) {
			sum2=sum2+arr[j];
		}
			 
}
		 System.out.println(sum1+" "+sum2);
        
		
	}
}